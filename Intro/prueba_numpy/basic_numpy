#location book 1103
import numpy as np

# create a 1D array
a = np.array([1,2,3])
#print(a)

# create a 2D array
b = np.array([[1,2,3],[4,5,6]])
#print(b)

# create a 1D array of zeros
a = np.zeros(3)
#print(a)

# create a 2D array of ones
# crea una matriz de tama√±o 2x3 (2 filas y 3 columnas) donde todos los elementos son iguales a 1
b = np.ones((2,3))
#print(b)

# create a 1D array of random numbers
c = np.random.rand(3)
#print(c)

